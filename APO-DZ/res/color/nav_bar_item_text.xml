<?xml version="1.0" encoding="utf-8"?><!-- Text Colors for Alumni Mode Navigation Bar List Items -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Checked -->
    <item android:color="@color/primary_dark" android:state_checked="true" />

    <!-- Unchecked -->
    <item android:color="@android:color/primary_text_light" android:state_activated="true" />

    <!-- Default -->
    <item android:color="@android:color/primary_text_light" />
</selector>